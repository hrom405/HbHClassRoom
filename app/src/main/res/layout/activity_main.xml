<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.hario.hbhclassroom.MainActivity"
    android:layout_margin="10dp">

  <Button
      android:id="@+id/Main_student"
      android:layout_width="200dp"
      android:layout_height="wrap_content"
      android:text="Student Login"
      android:layout_centerHorizontal="true"
      android:layout_marginTop="200dp"
      />
  <Button
      android:id="@+id/Main_faculty"
      android:layout_width="200dp"
      android:layout_height="wrap_content"
      android:text="Faculty Login"
      android:layout_marginTop="20dp"
      android:layout_below="@+id/Main_student"
      android:layout_centerHorizontal="true"/>

  <Button
      android:id="@+id/Main_notice"
      android:layout_width="200dp"
      android:layout_marginTop="20dp"
      android:layout_height="wrap_content"
      android:text="Notice"
      android:layout_below="@+id/Main_faculty"
      android:layout_centerHorizontal="true"
        />

</RelativeLayout>
